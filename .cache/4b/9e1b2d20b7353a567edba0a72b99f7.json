n      choice = (answerContainers[i].querySelector('input[name=question'+i+']:checked')||{}).value;\r\n      \r\n      if(choice===questions[i].correctAnswer){\r\n        numCorrect++;\r\n      }\r\n      else{\r\n        console.log('?')\r\n      }\r\n    }\r\n    resultsContainer.innerHTML = numCorrect + ' / ' + questions.length;\r\n  }\r\n\r\n  showQuestions(questions, quizContainer);\r\n  \r\n  submitButton.onclick = function(){\r\n    showResults(questions, quizContainer, resultsContainer);\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"154075ca98706f0dfae7d8508e31ab6e","cacheData":{"env":{}}}